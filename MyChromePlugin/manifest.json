{
	"manifest_version":2,	//chrome18以后需要加入
	"name":"够下载--测试",
	"version":"0.0.1",
	"description":"自动执行够下载",
	//"icons":{"icon.png"},//icons这里需要三种不同分辨率的图片，一般是16*16 48*48 128*128
	//"default_locale":"zh_CN",//支持国际化
	"browser_action":{
		"default_icon":"icon.png",//chrome18以后必须加入default_*
		"default_popup":"popup.html"
	},
	//"page_action":{},
	//"theme":{},
	//"app":{},
	//"background_page":"background.html",
	"background":{
		"scripts": ["js/background.js"]
	},
	//"chrome_url_overrides":{},//更换页
	"homepage_url":"http://www.ltaotao.net",
	//"incognito":"incognito",
	//"key":"publicKey",
	"minimum_chrome_version":"18",
	//"omnibox":{"keyword":"aString"},
	//"options_page":"",//选项的配置页
	"permissions":["tabs","http://goxiazai.cc/html*"],//需要使用权限
	 "content_scripts": [
	    {
	      "matches": ["http://goxiazai.cc/html*"],
	      "js": [ "js/down1.js"],
	      "run_at":"document_end"
	    }
	  ]
	//"plugins":[],//和dll交互的时候需要在这里配置
	//"update_url":""
}